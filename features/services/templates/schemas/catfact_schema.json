{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {
    "mensagens": {
      "type": "string"
    },
    "status": {
      "type": "boolean"
    },
    "data": {
      "type": "string",
      "pattern": "^[0-9:A-Z.-]*$"
    }
  },
  "type": "array",
  "minItems": 1,
  "maxItems": 5,
  "items": {
    "type": "object",
    "properties": {
      "status": {
        "type": "object",
        "properties": {
          "verified": {
            "type": "boolean",
            "enum": [
              true,
              false
            ]
          },
          "feedback": {
            "type": "string"
          },
          "sentCount": {
            "type": "integer",
            "enum": [
              1
            ]
          }
        },
        "additionalProperties": false,
        "required": [
          "verified",
          "sentCount"
        ]
      },
      "_id": {
        "$ref": "#/definitions/mensagens"
      },
      "user": {
        "$ref": "#/definitions/mensagens"
      },
      "text": {
        "type": "string",
        "pattern": "^[ '\"'a-z0-9A-Z:,?.-]*$"
      },
      "__v": {
        "type": "integer",
        "enum": [
          0
        ]
      },
      "source": {
        "type": "string",
        "enum": [
          "user"
        ]
      },
      "updatedAt": {
        "$ref": "#/definitions/data"
      },
      "type": {
        "type": "string",
        "enum": [
          "cat"
        ]
      },
      "createdAt": {
        "$ref": "#/definitions/data"
      },
      "deleted": {
        "$ref": "#/definitions/status"
      },
      "used": {
        "$ref": "#/definitions/status"
      }
    },
    "additionalProperties": true,
    "required": [
      "status",
      "_id",
      "user",
      "text",
      "__v",
      "source",
      "updatedAt",
      "type",
      "createdAt",
      "deleted",
      "used"
    ]
  }
}