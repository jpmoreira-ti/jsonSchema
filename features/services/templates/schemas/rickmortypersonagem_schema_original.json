{
    "$schema": "https://json-schema.org/draft-07/schema",
    "type": "object",
    "required": [
        "info",
        "results"
    ],
    "additionalProperties": true,
    "properties": {
        "info": {
            "type": "object",
            "required": [
                "count",
                "pages",
                "next",
                "prev"
            ],
            "additionalProperties": true,
            "properties": {
                "count": {
                    "type": "number"
                },
                "pages": {
                    "type": "number"
                },
                "next": {
                    "type": "string"
                },
                "prev": {
                    "type": "null"
                }
            }
        },
        "results": {
            "type": "array",
            "items": {
                "type": "object",
                "required": [
                    "id",
                    "name",
                    "status",
                    "species",
                    "type",
                    "gender",
                    "origin",
                    "location",
                    "image",
                    "episode",
                    "url",
                    "created"
                ],
                "additionalProperties": true,
                "properties": {
                    "id": {
                        "type": "number"
                    },
                    "name": {
                        "type": "string",
                        "pattern": "^[a-zA-Z (A-Z-0-9)]*$"
                    },
                    "status": {
                        "type": "string"
                    },
                    "species": {
                        "type": "string"
                    },
                    "type": {
                        "type": "string"
                    },
                    "gender": {
                        "type": "string"
                    },
                    "origin": {
                        "type": "object",
                        "required": [
                            "name",
                            "url"
                        ],
                        "additionalProperties": true,
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "url": {
                                "type": "string"
                            }
                        }
                    },
                    "location": {
                        "type": "object",
                        "required": [
                            "name",
                            "url"
                        ],
                        "additionalProperties": true,
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "url": {
                                "type": "string"
                            }
                        }
                    },
                    "image": {
                        "type": "string"
                    },
                    "episode": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "url": {
                        "type": "string"
                    },
                    "created": {
                        "type": "string"
                    }
                }
            }
        }
    }
}